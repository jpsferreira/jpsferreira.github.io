@inbook{AFONSO20231,
title = {Chapter 1 - Artificial intelligence: machine learning, deep learning, and applications in gastrointestinal endoscopy},
editor = {Miguel Mascarenhas and Hélder Cardoso and Guilherme Macedo},
booktitle = {Artificial Intelligence in Capsule Endoscopy},
publisher = {Academic Press},
pages = {1-10},
year = {\textbf{2023}},
isbn = {978-0-323-99647-1},
doi = {https://doi.org/10.1016/B978-0-323-99647-1.00003-4},
url = {https://www.sciencedirect.com/science/article/pii/B9780323996471000034},
author = {Jo{\~a}o Afonso and Miguel Martins and {\textbf{Ferreira, Jo{\~a}o P. S.}} and Miguel Mascarenhas},
keywords = {Artificial intelligence, gastroenterology, capsule endoscopy},
abstract = {Artificial intelligence (AI), in its various aspects, has revolutionized our world, and the healthcare field is no exception. The search for more effective, faster, and cheaper diagnostic and therapeutic methods has raised the interest in this type of technology, particularly in disciplines highly dependent on image analysis. Gastroenterology is one such area where the impact of AI technologies in clinical practice has been increasingly studied. In this chapter, we intend to explore the impact of AI in our world, with particular emphasis on advances in healthcare and gastroenterology.}
}

@inbook{Ferreira:2015ci,
  author        = {\textbf{Ferreira, J P S} and Parente, M P L and Jorge, R M Natal},
  editor    = {Tavares, João Manuel R. S. and Natal Jorge, R.M.},
  title     = {Injury Simulation of Anterior Cruciate Ligament Using Isogeometric Analysis},
  booktitle = {Computational and Experimental Biomedical Sciences: Methods and Applications},
  year          = {\textbf{2015}},
  doi           = {10.1007/978-3-319-15799-3_8},
  publisher = {Springer International Publishing},
  address   = {Cham},
  pages     = {105--121},
  abstract  = {Many active sportsmen trend to get injured in knee ligaments. For that reason, singular researches on the deformations limits of Anterior Cruciate Ligament (ACL) which ensure the stability of knee have been made. Understanding in detail the running of the knee plays an important role in the design of orthotics and prosthetics, and may also modify surgical protocols. As a consequence, the injury risk will be minimized with the right knowledge of the knee functioning. The Isogeometric Analysis has the potential to facilitate the study of the most of biomechanical models, which are usually geometrically complex. In this work, we present the deformation of ACL at an injury position using the isogeometric approach (IGA), suggested by Cottrell (Isogeometric analysis toward integration of CAD and FEA. Wiley, Hoboken, 2009) and Hughes et al. (Comput Methods Appl Mech Eng 194:4135--4195, 2005). Also is emphasized the potential of IGA formulations in future of the Biomechanics.},
  isbn      = {978-3-319-15799-3}
}
